package mainPack;

import java.util.Scanner;

public class questao_5 {

	public static void main(String[] args) {
		int soma = 0;
		
		System.out.print("O sistema soma os números entre 10 e 20, cujo resultado é: ");
		 do{
			if(soma > 10 || soma < 20) {
				soma = soma + soma;
			}
			soma++;
		}while(soma<=20);
		System.out.println(soma);
	}

}
